<span role="button">
	{{#io-ajax-upload
		parent=this
		name=name
		action=action
		headers=headers
		multipart=multipart
		dragdrop=_dragdrop
		withCredentials=withCredentials
		beforeUpload="beforeUpload"
		onStart="onStart"
		onProgress="onProgress"
		onSuccess="onSuccess"
		onError="onError"}} 
		{{yield}}
	{{/io-ajax-upload}}
</span>
